    "features": ["semantic_zoom", "overlays", "multi_scale", "drill_down"]
  },
  "navigation": {
    "levels": [
      {"id": "ecosystem", "name": "Ecosystem Overview", "description": "Full ALDC data ecosystem - 52 nodes across all layers"},
      {"id": "client", "name": "Client View", "description": "Client-specific data models and schemas"},
      {"id": "schema", "name": "Schema View", "description": "Tables, columns, and relationships"},
      {"id": "element", "name": "Element View", "description": "Individual data elements, measures, and metrics"}
    ],
    "zoom_targets": {
      "fusion92_client": {
        "target_graph": "f92_schema_graph.json",
        "target_endpoint": "/viz/f92",
        "level": "schema",
        "description": "Drill into Fusion92 Activation Model - 7 tables, 56 columns, 43 measures"
      },
      "gep_client": {
        "target_graph": "gep_schema_graph.json",
        "target_endpoint": "/viz/gep",
        "level": "schema",
        "description": "Drill into GEP/Navira e-commerce model"
      },
      "aldc_eng": {
        "target_graph": "aldc_schema_graph.json",
        "target_endpoint": "/viz/aldc",
        "level": "schema",
        "description": "Drill into ALDC Operations Model - 12 tables"
      },
      "snowflake_warehouse": {
        "target_graph": "snowflake_schemas.json",
        "level": "schema",
        "description": "Drill into Snowflake warehouse schemas"
      },
      "postgresql_zeus": {
        "target_graph": "zeus_memory_schema.json",
        "level": "schema",
        "description": "Drill into Zeus Memory database structure"
      }
    }
  },
  "overlays": {
    "projects": {
